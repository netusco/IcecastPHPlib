<icecast>
    <limits>
        <clients>100</clients>
        <sources>100</sources>
        <threadpool>10</threadpool>
        <queue-size>1638400</queue-size>
        <client-timeout>30</client-timeout>
        <header-timeout>15</header-timeout>
        <source-timeout>10</source-timeout>
        <burst-on-connect>0</burst-on-connect>
        <burst-size>128</burst-size>
    </limits>

    <authentication>
        <admin-user>streamingdjs</admin-user>
        <admin-password>dj489smn2011.</admin-password>
    </authentication>
    
    <location>Barcelona (Catalunya)</location> 
